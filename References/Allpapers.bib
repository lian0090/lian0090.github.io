	Language = {English},
	Number = {445},
	Pages = {pp. 247-253},
	Publisher = {Taylor & Francis, Ltd. on behalf of the American Statistical Association},
	Title = {Identifiability, Improper Priors, and Gibbs Sampling for Generalized Linear Models},
