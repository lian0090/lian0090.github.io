	Times-Cited = {{17}},
	Title = {{Hyperaccumulation of Zn by Thlaspi caerulescens can ameliorate Zn toxicity in the rhizosphere of cocropped Thlaspi arvense}},
	Unique-Id = {{ISI:000170281200044}},
	Volume = {{35}},
	Year = {{2001}},
