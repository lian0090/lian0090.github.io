	Issn = {{0008-4220}},
	Journal = {{CANADIAN JOURNAL OF PLANT SCIENCE}},
	Month = {{OCT}},
	Number = {{4}},
	Pages = {{803-823}},
	Times-Cited = {{8}},
	Title = {{The biology of Canadian weeds. 9. Thlaspi arvense L. (updated)}},
	Unique-Id = {{ISI:000179839400026}},
