	Number = {{4}},
	Pages = {{1655-1662}},
	Times-Cited = {{17}},
	Title = {{SELECTION AND CHARACTERIZATION OF A GIBBERELLIN-DEFICIENT MUTANT OF THLASPI-ARVENSE L}},
	Unique-Id = {{ISI:A1990EN70000023}},
