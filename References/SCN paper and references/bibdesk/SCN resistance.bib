@article{Lax:2002va,
	Abstract = {Cysts of Heterodera glycines races 1 and 3, collected in two localities in Argentina, were morphologically and morphometrically characterised. Although the morphological characters showed considerable variability, they were not useful in differentiating the populations. Most of the morphometrical characters showed a certain degree of variability and presented differences from values of other known populations of the species. No significant differences were observed between the mean values of the two populations, except for the characters' body length and body diameter.},
	Author = {Lax, P. and Doucet, M. E.},
	Date-Added = {2011-07-20 09:55:58 -0500},
