	Journal = {Journal of Nematology},
	Note = {PT: J; TC: 4},
	Number = {4},
	Pages = {402-408},
	Title = {Benomyl Tolerance of 10 Fungi Antagonistic to Plant-Parasitic Nematodes},
	Volume = {23},
	Year = {1991}}

@article{Li:1991ya,
	Abstract = {Race 4 of soybean cyst nematode (SCN) is highly pathogenetic to soybeans. In China, Huang, Huai and Hai soybean growing areas are the main places where it occurs and damages the crop. In this study, an evaluation of 8240 entries of soybean germplasm from six large areas of China to race 4 of SCN was carried out with standard grade system in China and a set of differential hosts which have been used in other countries. The results showed that no immune materials were found, while 9 entries from Shanxi, Hebei and Shaanxi provinces were high resistant to race 4 of SCN accounting for 0.1% of the total. All of the resistant materials belong to small grain soybean group with black seed coat in which Xingxian huipizhi heidou also has multiresistant ability to race 1, 3 and 5, this resistant material is seldom at home and in other countries. Four crosses from high resistant varieties .times. high susceptible varieties were made for resistant genetic analyses.},
