	Journal = {Crop Science},
	Keywords = {Plant Breeding and Genetics [FF020] Pests, Pathogens and Biogenic Diseases of Plants [FF600] Host Resistance and Immunity [HH600] Biotechnology (General) [WW000] biotechnology cluster analysis genetic resources grain legumes nematology pest resistance plant introduction plant nematology plant parasitic nematodes principal component analysis relationships resistance restriction fragment length polymorphism soyabeans Fabaceae Glycine (Fabaceae) Glycine max Heterodera glycines Nematoda invertebrates animals eukaryotes Papilionoideae Fabales dicotyledons angiosperms Spermatophyta plants Heterodera Heteroderidae eelworms. gene resources. plant. pulses. RFLP. Secernentea. soybeans. Tylenchida.},
	Note = {36 ref; Update Code: 20081216},
	Number = {6},
	Pages = {1966-1972},
	Title = {Genetic relationships among soybean plant introductions with resistance to soybean cyst nematodes},
	Volume = {37},
	Year = {1997}}

@article{Diers:1997dq,
	Abstract = {The results are presented of a study undertaken to confirm the mapping of the gene for resistance to Heterodera glycines on linkage group G using RFLP marker loci, and to map other genes from soyabean PI88788 conferring resistance to H. glycines races 3 and 14.},
