@article{Zhang:2006qi,
	Abstract = {Hirsutella rhossiliensis, a nematophagous fun.-us, has shown potential in biocontrol of plant-parasitic nematodes. Monitoring the population dynamics of a biocontrol agent in soil requires comprehensive techniques and is essential to understand how it works. Bioassay based on the fungal parasitism on the juveniles of soybean cyst nematode, Heterodera glycines, can be used to evaluate the activity of the fungus but fails to quantify fungal biomass in soil. A real-time polymerase chain reaction (PCR) assay was developed to quantify the fungal population density ill soil. The assay detected as little as 100 fg of fungal genomic DNA and 40 conidia g(-1) soil, respectively. The parasitism bioassay and the real-time PCR assay were carried out to investigate the presence, abundance and activity of H. rhossiliensis in soil after application of different inoculum levels. Both of the percentage of assay nematodes parasitized by H. rhossiliensis based on the parasitism bioassay and the DNA yield of the fungus quantified by real-time PCR increased significantly with the increase of the inoculum levels. The DNA yield of the fun-LIS was positively correlated with the percentage of assay nematodes parasitized by H. rhossiliensis. The combination of the two is useful for monitoring fungal biomass and activity in soil. (c) 2005 Elsevier Inc. All rights reserved.},
	Author = {Zhang, L. and Liu, X. Z. and Zhu, S. F. and Chen, S. Y.},
	Date-Added = {2011-07-20 09:55:58 -0500},
	Date-Modified = {2011-07-20 09:56:04 -0500},
	Journal = {Biological Control},
	Note = {PT: J; TC: 4},
	Number = {3},
	Pages = {316-323},
	Title = {Detection of the nematophagous fungus Hirsutella rhossiliensis in soil by real-time PCR and parasitism bioassay},
	Volume = {36},
	Year = {2006}}

@article{Zasada:2006vc,
	Abstract = {Velvetbean (Mucuna spp.) is a summer annual that has been used as a cover crop to reduce erosion, fix nitrogen and suppress weeds and plant-parasitic nematodes. Crude aqueous extracts (1: 15 dry weight plant/volume water) were made from velvetbean plant parts, and various concentrations of the extracts were evaluated in vitro for toxicities to different stages of Meloidogyne incognita (Kofoid and White) Chitwood and for suppression of hypocotyl. and root growth and inhibition of germination of tomato (Lycopersicon esculentum L.) and lettuce (Lactuca sativa L.). Germination was only affected by the full-strength extract from leaf blades. Lettuce root growth was the most sensitive indicator of allelopathic activity of the plant part extracts. Lettuce and tomato root growth was more sensitive to the extract from main roots than to extracts of other plant parts, with lethal concentration (LC50) values of 1.2 and 1.1% respectively. Meloidogyne incognita egg hatch was less sensitive to extracts from velvetbean than the juvenile (J2) stage. There was no difference among LC50 values of the extracts from different plant parts against the egg stage. Based on LC50 values, the extract from fine roots was the least toxic to J2 (LC50 39.9%), and the extract from vines the most toxic (LC50 7.8%). The effects of the extracts were nematicidal because LC50 values did not change when the extracts were removed and replaced with water. Published in 2006 by John Wiley & Sons, Ltd.},
	Author = {Zasada, I. A. and Klassen, W. and Meyer, S. L. F. and Codallo, M. and Abdul-Baki, A. A.},
	Date-Added = {2011-07-20 09:55:58 -0500},
	Date-Modified = {2011-07-20 09:56:07 -0500},
	Journal = {Pest management science},
	Note = {PT: J; TC: 2},
	Number = {11},
	Pages = {1122-1127},
	Title = {Velvetbean (Mucuna pruriens) extracts: impact on Meloidogyne incognita survival and on Lycopersicon esculentum and Lactuca sativa germination and growth},
	Volume = {62},
	Year = {2006}}

@article{Yamanaka:2006ci,
	Abstract = {Resistant varieties useful for stable soybean production in South America need to be developed, especially in light of outbreaks of pest and insect infestations, which are significant problems in this region. DNA markers can be used as precise and efficient tools for the introduction of resistance genes in plant breeding. The purpose of this study was to identify DNA markers that could be used in breeding programs for resistance against soybean diseases, and to use DNA markers to evaluate the genetic relationships among soybean genetic resources. We identified four quantitative trait loci (QTLs) associated with resistance against soybean sudden death syndrome (SDS) that explain nearly 50% of the phenotypic variation of the population used. The QTLs detected here differ from those previously identified for Fusarium virguliforme in North America. Three major genes for resistance to soybean rust, Rpp, Rpp2, and Rpp4, were mapped with DNA markers on linkage groups C2, J, and G, respectively. These maps were obtained by bulked segregant analysis (BSA) by using three independent F2 populations and their F3 and/or F4 progenies. Rpp2 and Rpp4 were located near regions where the genes for resistance to other fungal pathogens and genes for soybean cyst nematode resistance cluster. The DNA markers identified here can be used for the introduction of these resistance genes into soybean cultivars and for the accumulation of resistance genes into one line. Next, to evaluate the viability of introducing external gene pools to Brazilian soybeans, we evaluated the genetic relationships among Japanese, Chinese, and Brazilian soybean germplasms by using 12 DNA markers and a total of 261 varieties. Hayashi's Quantification Theory III (QT III) and the unweighted pair group method with arithmetic averages (UPGMA) clustering analysis revealed that the Chinese and Japanese varieties were classified as not independent but quite distant. The Brazilian varieties were distantly related to the Chinese and Japanese varieties and formed a cluster that was distant from the gene pools of Japan and China. Therefore, we expect that exchange of soybean genetic resources among these regions would expand their genetic bases and increase variability, especially when Brazilian varieties are used for soybean breeding in Japan and China or when Chinese varieties are used for breeding programs in Brazil.},
	Author = {Yamanaka, N. and Silva, D. C. G. da and Gilli, J. R. and Fuentes, F. H. and Yang, Z. and Polizel, A. and Sato, H. and Watanabe, S. and Ban, T. and Homma, Y. and Harada, K. and Brogin, R. L. and Nepomuceno, A. L. and Abdelnoor, R. V.},
	Date-Added = {2011-07-20 09:55:58 -0500},
	Date-Modified = {2011-07-20 09:56:03 -0500},
	Journal = {JIRCAS Working Report},
	Keywords = {Field Crops [FF005] Plant Breeding and Genetics [FF020] Viral, Bacterial and Fungal Diseases of Plants [FF610] Host Resistance and Immunity [HH600] Molecular Biology and Molecular Genetics [ZZ360] Taxonomy and Evolution [ZZ380] disease resistance fungal diseases genes genetic mapping genetic markers germplasm linkage phenotypic variation phylogenetics phylogeny plant diseases plant genetic resources plant pathogenic fungi plant pathogens quantitative trait loci soyabeans fungi Fusarium Glycine (Fabaceae) Glycine max South America America Developing Countries Threshold Countries Latin America East Asia Asia Developed Countries OECD Countries Deuteromycotina Eumycota eukaryotes Papilionoideae Fabaceae Fabales dicotyledons angiosperms Spermatophyta plants Brazil China Japan Fusarium tucumaniae. Fusarium virguliforme. Hyphomycetes. phenotypic variability. phytopathogens. resistance to disease. soybeans.},
	Note = {34 ref; Update Code: 20081216},
	Number = {51},
	Pages = {47-53},
	Title = {Application of DNA markers for identifying genes for resistance to soybean diseases in South America and for evaluating genetic relationships among soybean gene pools. (Comprehensive studies on the development of sustainable soybean production technology in South America)},
	Year = {2006}}

@article{Xing:2006fh,
	Abstract = {Sudden death syndrome (SIDS) of soybean is caused by the soilborne Fusarium solani f. sp. glycines (synonym F. virguliforme). In a sequential approach, two multifactor factorial-design microplot experiments were conducted to investigate the effects of fungal infestation levels and soil moisture on both root necrosis and foliar SDS severity, and the interaction between F. solani f. sp. glycines and Heterodera glycines in fumigated versus nonfumigated soil. In 2003, soybean cv. Spencer was grown in fumigated or methyl bromide-fumigated soil and infested with increasing levels of F. solani f. sp. glycines, either under rainfall or irrigated after growth stage V6/R1. In 2004, interactions between F. solani f. sp. glycines and H. glycines were explored in a factorial inoculation design in fumigated or nonfumigated soil, planted to Williams 82 or Cyst-X20-18. In both years, higher levels of foliar SIDS severity and root necrosis were found in F. solani f. sp. glycines-infested soils with H. glycines than in soils without the nematode on the soybean cultivars susceptible to both pathogens. Both natural infestations of H. glycines in 2003 and artificially amended populations of H. glycines in 2004 contributed to higher foliar SIDS severity. More severe foliar SIDS symptoms always were associated with more root necrosis. but elevated levels of root necrosis did not predict severe leaf symptoms. In contrast to the critical role of H. glycines, increasing fungal infestation levels had no significant effects on increasing either foliar SDS symptoms or root necrosis. Effects of moisture regime and fungal infestation levels also were examined in factorial greenhouse and growth chamber experiments. High soil moisture resulted in higher levels of SIDS root necrosis. In the greenhouse, root necrosis increased at a higher rate in low soil moisture than the rate in high soil moisture. The two pathogens acted as a complex and the disease development was strongly dependent on high soil moisture.},
	Author = {Xing, L. J. and Westphal, A.},
	Date-Added = {2011-07-20 09:55:58 -0500},
	Date-Modified = {2011-07-20 09:56:05 -0500},
	Journal = {Phytopathology},
	Note = {PT: J; TC: 4},
	Number = {7},
	Pages = {763-770},
	Title = {Interaction of Fusarium solani f. sp glycines and Heterodera glycines in sudden death syndrome of soybean},
	Volume = {96},
	Year = {2006}}

