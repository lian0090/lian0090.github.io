	Journal = {International journal for parasitology},
	Note = {PT: J; TC: 13},
	Number = {12},
	Pages = {1377-1383},
	Title = {Molecular characterisation and developmental expression of a cellulose-binding protein gene in the soybean cyst nematode Heterodera glycines},
	Volume = {34},
	Year = {2004}}

