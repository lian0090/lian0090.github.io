@article{Yuan:2008by,
	Abstract = {Soybean cyst nematode (SCN) (Heterodera glycines Ichinohe) is the most important pathogen in soybean production worldwide and causes substantial yield losses. An apparent narrow genetic base of SCN resistance was observed in current elite soybean cultivars, and searching for novel SCN resistance genes as well as novel resistance sources rather than focusing on the two important genes rhg1 and Rhg4 has become another major objective in soybean research. In the present paper we report a 1,477 bp Hs1(pro-1) homolog, named GmHs1(pro-1). This gene was cloned from soybean variety Wenfeng 7 based on information for Hs1(pro-1), a beet cyst nematode resistance gene in sugar beet. It has two domains, Hs1pro-1_N and Hs1pro-1_C, both of which are believed to confer resistance to nematodes. Of the 1,477 bp sequence in GmHs1(pro-1), an open reading frame of 1,314 bp, encoding a protein with 437 amino acids, was flanked by a 5'-untranslated region of 27 bp and a 3'-untranslated region of 135 bp. Fourteen single-nucleotide polymorphisms (SNPs) were observed in 44 soybean accessions including 23 wild soybeans, 8 landraces, and 13 soybean varieties (or lines), among which 5 in wild soybeans and 3 in landrace accessions were unique. Sequence diversity analysis on the 44 soybean accessions showed pi = 0.00168 and theta = 0.00218 for GmHs1(pro-1); landraces had the highest diversity, followed by wild soybeans, with varieties (or lines) having the lowest. Although we did not detect a significant effect of selection on GmHs1(pro-1) in the three populations, sequence diversity, unique SNPs, and phylogenetic analysis indicated a slight domestication bottleneck and an intensive selection bottleneck. High sequence diversity, more unique SNPs, and broader representation across the phylogenetic tree in wild soybeans and landraces indicated that wild collections and landrace accessions are invaluable germplasm for broadening the genetic base of elite soybean varieties resistant to SCN.},
	Author = {Yuan, Cuiping and Zhou, Guoan and Li, Yinghui and Wang, Kejing and Wang, Zhi and Li, Xianghua and Chang, Ruzhen and Qiu, Lijuan},
	Date-Added = {2011-07-20 09:55:58 -0500},
	Date-Modified = {2011-07-20 09:56:04 -0500},
	Journal = {Molecular Breeding},
	Keywords = {[03502] Genetics - General [03504] Genetics - Plant [03506] Genetics - Animal [36001] Medical and clinical microbiology - General and methods [52502] Agronomy - Miscellaneous and mixed crops [52514] Agronomy - Oil crops [64016] Invertebrata: comparative, experimental morphology, physiology and pathology - Aschelminthes Infection Methods and Techniques Molecular Genetics: Biochemistry and Molecular Biophysics Agronomy: Agriculture [25795] Chenopodiaceae [26260] Leguminosae [51300] Nematoda [25795] Chenopodiaceae, Dicotyledones, Angiospermae, Spermatophyta, Plantae [26260] Leguminosae, Dicotyledones, Angiospermae, Spermatophyta, Plantae [51300] Nematoda, Aschelminthes, Helminthes, Invertebrata, Animalia Chenopodiaceae: Angiosperms, Dicots, Plants, Spermatophytes, Vascular Plants Leguminosae: Angiosperms, Dicots, Plants, Spermatophytes, Vascular Plants Nematoda: Animals, Aschelminths, Helminths, Invertebrates sugar beet: common [Chenopodiaceae] Glycine max: species [Leguminosae] Glycine soja: species [Leguminosae] soybean: common, host, oil crop, cultivar-Suinong 14, cultivar-Suiheidadou, cultivar-Jin 1267, cultivar-Jinpin 42, cultivar-Nenfeng 15, cultivar-Kangxian 3, cultivar-You 1298, cultivar-You 232, cultivar-Jindou 11, cultivar-Jinhuang 138, cultivar-Zaoshu Heterodera glycines: species, soybean cyst nematode, common, pathogen [Nematoda] Heterodera glycines infection: infectious disease soybean Rhg4 gene [Leguminosae] soybean rhg1 gene [Leguminosae] soybean GmHs1pro-1 gene [Leguminosae] domestication bottleneck intensive selection bottleneck},
	Note = {Record Owner: Copyright Thomson 2008.; Methods & Equipment: cloning: laboratory techniques, genetic techniques; phylogenetic analysis: mathematical and computer techniques; sequence diversity analysis: laboratory techniques, genetic techniques; Update Code: 200849. BIOSIS Update: 20081111.},
	Number = {4},
