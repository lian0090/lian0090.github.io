	Journal = {Theoretical and Applied Genetics},
	Note = {PT: J; TC: 1},
	Number = {6},
	Pages = {857-871},
	Title = {Genetic structure and diversity of cultivated soybean (Glycine max (L.) Merr.) landraces in China},
	Volume = {117},
	Year = {2008}}

