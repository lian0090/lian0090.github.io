@article{Demuner:2001je,
	Abstract = {In the search for new synthetic nematicides, eight piperazine derivatives were prepared. These compounds, and also two commercially available piperazine derivatives and piperazine, were submitted to a biological essay to evaluate their activity against the phytonematodes Meloidogyne incognita and Heterodera glycines, At the concentration of 20 g mL(-1), the compounds caused 3 to 73% mortality of M. incognita and 7 to 58% mortality of H. glycines.},
