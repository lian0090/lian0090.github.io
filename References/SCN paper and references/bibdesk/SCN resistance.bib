@article{Chen:2004yi,
	Abstract = {Eight soyabean cultivars (6 and 2 cultivars resistant and susceptible, respectively, to the SCN race 3 of soyabean cyst nematode, Heterodera glycines) were subjected to polymerase chain reaction (PCR) using a pair of primers developed for H. glycines resistance gene in soyabean. The results of PCR analysis were similar to those obtained by conventional gene identification. These results suggest that PCR-Southern blot is a feasible, simple, rapid and reliable method for the detection of the H. glycines resistance gene in soyabean.},
