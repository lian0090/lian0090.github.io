	Note = {PT: J; CT: INTERNATIONAL SYMP ON ADVANCES IN POTATO CROP PROTECTION; CY: SEP 01-06, 1991; CL: WAGENINGEN, NETHERLANDS; TC: 0},
	Number = {Journal Article},
	Pages = {175-181},
	Title = {Molecular Events at Nematode-Induced Feeding Sites},
	Volume = {98},
	Year = {1992}}

@article{Ying:1991zj,
	Abstract = {An evaluation of 8240 entries of soyabean germplasm from 6 large areas of China to race 4 of Heterodera glycines, the soyabean cyst nematode (SCN), was carried out in pot experiments. No immune materials were found, while 9 entries from Shanxi, Hebei and Shaanxi provinces were highly resistant, accounting for 0.1% of the total. All of the resistant materials belonged to the small seeded soyabean group with black seed coat, in which Xingxian huipizhi heidou showed multiple resistance to races 1, 3 and 5. Four crosses between highly resistant and highly susceptible varieties were made for resistance genetics analysis.},
