	Journal = {Plant Disease},
	Note = {PT: J; TC: 8},
	Number = {6},
	Pages = {594-595},
	Title = {A Rapid Method for Inoculating Soybean Seedlings with Heterodera-Glycines},
	Volume = {75},
	Year = {1991}}

@article{RaoArelli:1991pp,
	Abstract = {Resistance in soyabeans to Heterodera glycines race 3 was evaluated in the F1-F3 of resistant (R) cv. Peking X susceptible (S) cv. Essex, PI90763 (R) X Essex and PI90763 X PI88788 (R). Segregation ratios indicated a 3-gene model for the resistant parents, Peking and PI90763 having 1 dominant (Rhg) and 2 recessive (rhg) resistance genes and PI88788 having 1 recessive and 2 dominant genes. The second recessive in Peking and PI90763 and the additional dominant gene in PI88788 appeared to provide an epistatic interaction.},
