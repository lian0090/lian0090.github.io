@article{Achenbach:1996xg,
	Abstract = {Fusarium solani is the etiological agent of soybean sudden death syndrome (SDS). Random amplified polymorphic DNA (RAPD) analysis was used to differentiate isolates that cause SDS from those that do not. Greenhouse assays with a sand/cornmeal inoculum method were performed to assess virulence of the fungal isolates and the results were compared with the RAPD pattern analysis. The RAPD results exhibited 100% association with greenhouse virulence assay results. Amplification patterns generated by RAPD reactions were used to generate a phenogram depicting the genetically distinct nature of the cluster defined by the SDS isolates. This cluster was exclusive and distinct from F. solani f. sp. phaseoli isolates that do not cause SDS. Thus, the RAPD approach proved a sensitive and highly reliable method for quickly identifying fungal pathotypes that cause soybean SDS.},
	Author = {Achenbach, L. A. and Patrick, J. and Gray, L.},
	Date-Added = {2011-07-20 09:55:58 -0500},
	Date-Modified = {2011-07-20 09:56:07 -0500},
	Journal = {Plant Disease},
	Note = {PT: J; TC: 32},
	Number = {11},
	Pages = {1228-1232},
	Title = {Use of RAPD markers as a diagnostic tool for the identification of Fusarium solani isolates that cause soybean sudden death syndrome},
	Volume = {80},
	Year = {1996}}

@article{Zhang:1995rc,
	Abstract = {Population densities of Meloidogyne konaensis were determined in March and July of 1991 and 1992 on coffee cultivars Guatemalan and 502, and on four rootstocks (Purpuree, Congensis, Deweveri, and Kaffe) with Guatemalan or 502 as a scion. Three-dimensional spatial patterns were characterized on roots of Guatemalan and Deweveri. Population densities differed among rootstocks (P < 0.05) and times (P < 0.01). The greatest number of second-stage juveniles (J2) occurred on Guatemalan and fewest J2 on Purpuree and Deweveri rootstocks. More nematodes were found in March than in July of both years. The spatial distribution varied by positions and depths on Guatemalan. The highest nematode population density occurred at 60 cm from the base of the tree and 15-45 cm deep. Numbers of nematodes were relatively low at all positions and all depths on the Guatemalan-Deweveri combination.},
	Author = {Zhang, F. R. and Schmitt, D. P.},
	Date-Added = {2011-07-20 09:55:58 -0500},
	Date-Modified = {2011-07-20 09:56:06 -0500},
	Journal = {Journal of Nematology},
	Note = {PT: J; TC: 3},
	Number = {1},
	Pages = {109-113},
	Title = {Spatial-Temporal Patterns of Meloidogyne Konaensis on Coffee in Hawaii},
	Volume = {27},
	Year = {1995}}

@article{Zhang:1995pb,
	Abstract = {The effects of temperature on embryogenesis and postinfection development in Meloidogyne konaensis were examined. Embryogenesis was evaluated at 5, 8, 10, 13, 16, 20, 22, 24, 26, 28, 30, 35, and 40 C. No embryonic development occurred at 5 C. Some development, although incomplete, occurred at 8 and 10 C. The rate of embryogenesis was linear from 13 to 30 C, but decreased at 35 C. The lowest egg mortality occurred at 24 C, whereas all eggs died within 24 hours at 40 C. Postinfection development was determined on coffee and tomato in a greenhouse at an average temperature of 30 C and in a growth chamber with a constant temperature of 26 C. Development of M. Konaensis J2 to mature female required 38 and 48 days on coffee at 30 and 26 C, respectively. This process took 20 and 26 days, respectively, on tomato.},
	Author = {Zhang, F. R. and Schmitt, D. P.},
	Date-Added = {2011-07-20 09:55:58 -0500},
	Date-Modified = {2011-07-20 09:56:04 -0500},
	Journal = {Journal of Nematology},
	Note = {PT: J; TC: 2},
	Number = {1},
	Pages = {103-108},
	Title = {Embryogenesis and Postinfection Development of Meloidogyne Konaensis},
	Volume = {27},
	Year = {1995}}

@article{Young:1995ig,
	Abstract = {Soybean, Glycine maw (L.) Merr., accessions added to the U.S. soybean germplasm collection since 1980 were evaluated for reaction to Races 3, 5, and 14 of the soybean cyst nematode, Heterodera glycines Ichinohe. Accessions were evaluated in the greenhouse by inoculation with approximately 1000 H. glycines eggs and enumeration of females developing on roots 35 d after planting. Nineteen accessions were resistant to Race 3, 11 accessions were resistant to Race 5, and two accessions were resistant to Race 14. The PI 467312 was resistant to all three races. These resistant accessions may contain new genes for resistance to this nematode.},
	Author = {Young, L. D.},
	Date-Added = {2011-07-20 09:55:58 -0500},
