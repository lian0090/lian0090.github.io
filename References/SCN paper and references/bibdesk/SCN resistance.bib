	Journal = {Pesquisa Agropecuaria Brasileira},
	Note = {PT: J; TC: 0},
	Number = {2},
	Pages = {271-275},
	Title = {Bootstrap confidence intervals as tool to classify races of soybean's cyst nematode},
	Volume = {35},
	Year = {2000}}

