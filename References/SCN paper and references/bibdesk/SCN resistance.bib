@article{Lewers:2001fb,
	Abstract = {The soybean cyst nematode (SCN), Heterodera glycines Ichinohe, is the foremost pest of soybean (Glycine max L. Merr.). The rhg1 allele on linkage group (LG) G and the Rhg4 allele on LG A2 are important in conditioning resistance. Markers closely linked to the Rhg4 locus were used previously to screen a library of bacterial artificial chromosome (BAC) clones from susceptible 'Williams 82' and identified a single 150-kb BAC, Gm-ISb001-056-G02 (56G2). End-sequenced subclones positioned onto a restriction map provided landmarks for identifying the corresponding region from a BAC library from accession PI 437654 with broad resistance to SCN. Seventy-three PI 437654 BACs were assigned to contigs based upon HindIII restriction fragment profiles. Four contigs represented the PI 437654 counterpart of the 'Williams 82' BAC, with PCR assays connecting these contigs. Some of the markers on the PI 437654 contigs are separated by a greater physical distance than in the 'Williams 82' BAC and some primers amplify bands from BACs in the mid-portion of the connected PI 437654 BAC contigs that are not amplified from the 'Williams 82' BAC. These observations suggest that there is an insertion in the PI 437654 genome relative to the 'Williams 82' genome in the Rhg4 region.},
	Author = {Lewers, K. S. and Nilmalgoda, S. D. and Warner, A. L. and Knap, H. T. and Matthews, B. F.},
	Date-Added = {2011-07-20 09:55:58 -0500},
	Date-Modified = {2011-07-20 09:56:06 -0500},
	Journal = {Genome},
	Keywords = {[03502] Genetics - General [03504] Genetics - Plant [03506] Genetics - Animal [52502] Agronomy - Miscellaneous and mixed crops [52514] Agronomy - Oil crops [60502] Parasitology - General [64016] Invertebrata: comparative, experimental morphology, physiology and pathology - Aschelminthes Agronomy: Agriculture Molecular Genetics: Biochemistry and Molecular Biophysics Parasitology [26260] Leguminosae [51300] Nematoda [26260] Leguminosae, Dicotyledones, Angiospermae, Spermatophyta, Plantae [51300] Nematoda, Aschelminthes, Helminthes, Invertebrata, Animalia Leguminosae: Angiosperms, Dicots, Plants, Spermatophytes, Vascular Plants Nematoda: Animals, Aschelminths, Helminths, Invertebrates Glycine max: soybean, cultivar-Williams 82, host, oil crop [Leguminosae] Heterodera glycines: soybean cyst nematode, parasite [Nematoda] Glycine max Rhg-4 gene [Leguminosae]: nematode resistance BAC library [bacterial artificial chromosome library] parasite resistance},
	Note = {Record Owner: Copyright BIOSIS 2003.; Methods & Equipment: polymerase chain reaction assay: molecular genetic method; Update Code: 200203. BIOSIS Update: 20020116.},
	Number = {6},
	Pages = {1057-1064},
	Title = {Physical mapping of resistant and susceptible soybean genomes near the soybean cyst nematode resistance gene Rhg4},
	Volume = {44},
	Year = {2001}}

