@inproceedings{Young:zr,
	Abstract = {Resistance to soyabean cyst nematode (SCN; Heterodera glycines) is complex and involves multiple unlinked genes. To identify genes for SCN resistance, 4 distinct soyabean F2 populations and one recombinant inbred population segregating for resistance were analysed using RFLP markers. Among all the populations, 4 independent loci were significantly associated with SCN resistance. One of these loci, located in linkage group G, behaved as a major partial resistance gene and was common to all the populations studied. This locus explained at least 49% of the total variation in resistance (based on r-square estimates) in one population (PI209332 x Evans) and between 23 and 45% of the variation in resistance in the remaining populations. The locus mapped to a specific interval defined by two flanking RFLP markers (Bng173 and Bng122) approximately 12 cM apart. This information was used to characterize 147 F4 and F5 lines derived from 16 crosses. The lines were assayed in parallel in greenhouse SCN tests. Selection for the resistant parent allele on the basis of DNA markers identified lines with, on average, a 43% decrease in SCN index. Moreover, marker selection uncovered lines with interesting crossovers very tightly linked to the SCN resistance gene. These crossovers may lead to lines with increased yield, together with high levels of resistance.},
	Address = {Vienna},
	Author = {Young, N. D. and Denny, R. L. and Concibido, V. C. and Lange, D. A. and Orf, J. H.},
	Booktitle = {Induced mutations and molecular techniques for crop improvement. Proceedings},
