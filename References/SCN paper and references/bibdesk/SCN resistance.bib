@article{Yuan:2008pr,
	Abstract = {Soyabean cyst nematode (SCN, Heterodera glycines) is a seriously destructive pest in soyabean production worldwide and causes great yield loss every year. Planting resistant variety is an effective and economical method to decrease its damage. At present we obtained 41 elite soyabean lines from the crosses of Hartwig x Jin 1261, Hartwig x Jin 1265 and Hartwig x Jin 1267, and genotyped them with 130 SSR markers from 20 linkage groups of soyabean, to provide new soyabean germplasm as well as molecular markers used in soyabean breeding for resistance to SCN. The results suggest that Hartwig and Jin 1261, Jin 1265 or Jin 1267 had different resistance genes to SCN 4, with genetic similarity of 0.362. Twenty-two SSR markers from 11 linkage groups were associated with resistance to SCN 4, and at Satt684, Sat_230, Sat_222, Satt615 and Satt231 loci, the allele from Hartwig was resistant type, while the allele from Jin 1261, Jin 1265 and Jin 1267 was susceptible. On the other hand, at the other 17 SSR loci, the allele from Hartwig was susceptible, while the allele from Jin 1261, Jin 1265 and Jin 1267 was resistant. We supposed that Jin strains had at least a new resistance gene on linkage group D1b. We could effectively identify the resistance of these soyabean lines to SCN 4 with Satt684 and Sat_400.},
	Author = {Yuan, CuiPing and Lu, WeiGuo and Liu, ZhangXiong and Li, YingHui and Li, WeiDong and Guan, RongXia and Chang, RuZhen and Qiu, LiJuan},
	Date-Added = {2011-07-20 09:55:58 -0500},
